    <div class="skill-section">
      <div class="skill-section-content">
        <h1 class="section-headline">Getting skilled has <br> never been easier</h1>
        <p>A one-stop skill development educational <br> platform for school and college students.</p>
        <div class="skill-section-btns">
          <button>All programs</button>
          <button>Why Edudictive</button>
        </div>
      </div>
      <img src="/assets/images/svgs/homepage-hero.svg" alt="">
    </div>
    <!-- Program Section -->
    <div class="program-section">
      <h1 class="section-headline">Programs designed specially for you</h1>
      <div class="course-wrapper">
        <% for(let i=0; i<10; i++) { %>
          <div class="course-card">
            <div class="date-wishlist"><span>Starting May 1</span><span><img src="/assets/images/svgs/heart.svg" alt=""></span></div>
            <div class="course-name-wrapper">
              <span class="course-name">Front-end Web Development</span>
              <span class="course-mode">Offline Training Program</span>
            </div>
            <div class="ratings">
              <div class="rating-stars">
                <img src="/assets/images/svgs/Filled-Star.svg" alt="">
                <img src="/assets/images/svgs/Filled-Star.svg" alt="">
                <img src="/assets/images/svgs/Filled-Star.svg" alt="">
                <img src="/assets/images/svgs/Filled-Star.svg" alt="">
                <img src="/assets/images/svgs/half filled star.svg" alt="">
              </div>
              <span>4.5 (90 Ratings)</span>
            </div>
            <div class="explore-course">
              <button>Learn More</button>
              <button>Register Now</button>
            </div>
          </div>
        <% } %>
        <div class="extra-space"></div>
      </div>
      <div class="program-section-btns">
        <button><i class="fa fa-chevron-left"></i></button>
        <button><i class="fa fa-chevron-right"></i></button>
      </div>
      <div class="position-points">
        <% for(let i=0; i<10; i++) { %>
          <div class="course-pointer"></div>
        <% } %>
      </div>
    </div>
    <!-- Institute Section -->
